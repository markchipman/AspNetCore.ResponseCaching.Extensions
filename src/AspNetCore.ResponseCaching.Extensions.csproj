<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/speige/AspNetCore.ResponseCaching.Extensions/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/speige/AspNetCore.ResponseCaching.Extensions</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>csharp, asp, net, core, standard, response, cache, caching, disk, disc, memory, output</PackageTags>
    <Authors>Devin Garner</Authors>
    <Company />
    <Description>Extensions for overriding the behaviour of the default ASP.Net Core ResponseCaching as well as additional Cache implementations such as Disk. (Similar to OutputCache of previous versions of ASP.Net)</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.ResponseCaching" Version="2.0.1" />
  </ItemGroup>

</Project>
